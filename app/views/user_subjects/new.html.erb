<div class="col-md-6 col-md-offset-3">
  <%= form_for @user_subject do |f| %>
    <%= render "shared/error_messages", object: f.object %>
    <%= f.label :user_id %>
    <%= f.number_field :user_id, class: "form-control" %>

    <%= f.label :subject_id %>
    <%= f.number_field :subject_id, class: "form-control" %>

    <%= f.label :status %>
    <div class="col-md-4">
      <%= f.select :status, Settings.subject.user %>
    </div>
    <%= f.submit t("submit"), class: "btn btn-primary" %>
    <% end %>
</div>
