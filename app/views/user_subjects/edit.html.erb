<div class="col-md-6 col-md-offset-3">
  <%= form_for @user_subject do |f| %>
    <%= render "shared/error_messages", object: f.object %>
    <%= f.label :status %>
    <div class="col-md-4"> 
      <%= f.select :status, Settings.subject.user %>
    </div>
    <%= f.submit t("submit"), class: "btn btn-primary" %>
    <% end %>
</div>
