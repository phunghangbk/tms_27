<div class="col-md-6 col-md-offset-3">
  <%= form_for @user_task do |f| %>
    <%= render "shared/error_messages", object: f.object %>
    <%= f.label :user_id %>
    <%= f.number_field :user_id, class: "form-control" %>

    <%= f.label :task_id %>
    <%= f.number_field :task_id, class: "form-control" %>

    <%= f.label :finish %>
    <div class="col-md-4">
      <%= f.select :finish, Settings.subject.user %>
    </div>
    <%= f.submit t("submit"), class: "btn btn-primary" %>
  <% end %>
</div>
